{
  "name": "@repo/configmanager",
  "version": "1.0.3",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist es lib",
    "build": "npm run clean && tsup",
    "dev": "nodemon --watch src --ext ts,tsx,css,json,js,jsx --exec \"npm run build\"",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "nodemon": "^3.1.3",
    "@types/node": "^20.11.24",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
